{
    "name": "acelords/laravel-docsify",
    "description": "Add Docsify to Laravel",
    "authors": [
        {
            "name": "AceLords",
            "email": "info@acelords.space"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../packages/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "AceLords\\Docsify\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AceLords\\Docsify\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AceLords\\Docsify\\DocsifyServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "~3.7"
    }
}
